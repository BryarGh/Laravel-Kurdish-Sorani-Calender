{
    "name": "bryarghafoor/sorani-calendar",
    "description": "Laravel package for converting Gregorian dates to Sorani Kurdish calendar",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "kurdish",
        "sorani",
        "calendar",
        "kurdish-calendar",
        "date-conversion"
    ],
    "authors": [
        {
            "name": "Bryar Ghafoor",
            "email": "bryar@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "BryarGhafoor\\SoraniCalendar\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BryarGhafoor\\SoraniCalendar\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BryarGhafoor\\SoraniCalendar\\SoraniCalendarServiceProvider"
            ],
            "aliases": {
                "SoraniCalendar": "BryarGhafoor\\SoraniCalendar\\Facades\\SoraniCalendar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
